nuno-card-name = {$suit} {$card}
nuno-card-desc = Such a simple but fun game!

nuno-card-suit-name = { $suit ->
    [nunored] Red
    [nunoyellow] Yellow
    [nunogreen] Green
    [nunoblue] Blue
   *[invalid] !!{$suit}!!
}

nuno-card-value-name = { $card ->
    [plus2] Plus 2
    [reverse] Reverse
    [skip] Skip
   *[other] {$card}
}

